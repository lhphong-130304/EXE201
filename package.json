{
	"scripts": {
		"test": "echo \"Error: no test specified\" && exit 1",
		"preinstall": "npx npm-force-resolutions",
		"dev": "npx @tailwindcss/cli -i ./src/assets/css/tailwind.css -o ./src/assets/css/style.css --watch",
		"build": "npx @tailwindcss/cli -i ./src/assets/css/tailwind.css -o ./src/assets/css/style.css",
		"server": "live-server src --port=3000",
		"backend": "dotnet run --project GymFinder.Api",
		"start": "npm-run-all --parallel dev server backend",
		"stop": "taskkill /F /IM node.exe /IM dotnet.exe"
	},
	"dependencies": {
		"@tailwindcss/cli": "^4.0.14"
	},
	"devDependencies": {
		"live-server": "^1.2.2",
		"npm-run-all": "^4.1.5",
		"tailwindcss": "^4.0.14"
	}
}